     "output_type": "execute_result"
    }
   ],
   "source": [
    "function trapezoid(f, a, b, n)\n",
    "    h = (b-a) / n\n",
    "    domain = a:h:b\n",
    "    \n",
    "    pairs = zip(domain[1:end-1], domain[2:end])\n",
    "    sum(h * (f(an) + f(bn)) / 2 for (an, bn) in pairs)\n",
    "end"
   ]
  },
